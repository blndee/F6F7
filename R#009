const chs = [
  "1",
  "2",
  "3",
  "4",
  "5",
  "6",
  "7"
];

if (chs.includes("Waldo")) {
  let waldoIndex = chs.indexOf("Waldo");
  console.log(`Found Waldo at index ${waldoIndex}!`)
} else {
  console.log("Not found")
}

